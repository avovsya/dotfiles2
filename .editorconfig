root = true

[**.js]
indent_style = space
indent_size = 2
bin=node
indent_style = space
indent_size = 2
jslint_happy = true
preserve_newlines = true
max_preserve_newlines = 2
space_in_paren = false
brace_style = collapse
keep_array_indentation = false
keep_function_indentation = false
eval_code = false
unescape_strings = false
break_chained_methods = false


[**.css]
indent_style = space
indent_size = 4

[**.html]
indent_style = space
indent_size = 4
max_char = 78
brace_style = expand
